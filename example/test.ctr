import generate: \*.

model City [
  string country,
  string city,
  int countryCode,
  int cityCode
].

model Person [
  string firstName,
  string lastName,
  reference[City] city
].

action getPeopleFromCity [
    Request City,
    Response [
        Person[] person
    ],
    Action 'search for person in db'
].

action deletePerson [
    Request [
        int personId
    ],
    Action ['get user from db',
            'delete user']
].

endpoint countUsers [
    Response [
        int count
    ]
].

api generate.
